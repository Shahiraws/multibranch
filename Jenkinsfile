
pipeline{
agent any
	stages{
		stage('SCM'){
			steps{
           echo 'Iam scm'
             }
        } 
		stage('Build'){
            steps{
                echo 'Iam build'
            }
        }

		stage('Test'){
            steps{
                echo 'Iam test'
            }
            }    	
    stage('Deploy'){
            steps{
                echo 'Iam deploy'
            }
        }  
    
    }
} 
